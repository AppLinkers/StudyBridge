<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studybridge">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudyBridge"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Subscribe.PaymentActivity"
            android:exported="true" />
        <activity
            android:name=".AtuhActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".ToDo.ToDoDetailActivity"
            android:exported="true" />
        <activity
            android:name=".Study.StudyAddActivity"
            android:exported="true" />
        <activity
            android:name=".Subscribe.SubscribeActivity"
            android:exported="true" />

        <activity android:name=".Study.StudyMentiDetail"
            android:exported="true"/>

        <activity android:name=".Study.StudyMentoDetail"
            android:exported="true"/>
        <activity android:name=".Study.StudyMentiFilterActivity"
            android:exported="true"/>
        <activity
            android:name=".Login.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>